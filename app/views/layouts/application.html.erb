  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div id="banner">
  <%= image_tag("logo.png") %>
  <%= @page_title || "Sunday League" %>
</div>
<div id="columns">
  <div id="side">
    <ul>
	<li><a href="http://localhost:3000/teams">Home</a></li>
	<li><a href="http://localhost:3000/table/index">Table</a></li>
    </ul>
  </div>
  <div id="main">
    <%= yield %>
  </div>
</div>

</body>
</html>
